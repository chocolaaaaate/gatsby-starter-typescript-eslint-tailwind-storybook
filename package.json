{
  "name": "sk-portfolio",
  "description": "Sumedh's Portfolio",
  "version": "4.0.0",
  "author": "Sumedh Kanade",
  "dependencies": {
    "gatsby": "2.24.65",
    "gatsby-plugin-manifest": "2.4.29",
    "gatsby-plugin-offline": "3.2.27",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-react-helmet": "3.3.11",
    "gatsby-plugin-sharp": "2.6.35",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-text-transition": "^1.1.2"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "tailwind",
    "tailwindcss",
    "storybook",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .tsx --ext .ts --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,ts,tsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "type-check": "./node_modules/typescript/bin/tsc --noEmit --skipLibCheck",
    "fix:lint": "eslint --ext .jsx --ext .tsx --ext .ts --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,ts,tsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "npm run type-check && gatsby build",
    "develop": "npm run type-check && gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.21",
    "@tailwindcss/custom-forms": "0.2.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.9.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.6",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-typescript": "^2.4.21",
    "postcss-import": "^12.0.1",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "tailwindcss": "1.8.10",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
